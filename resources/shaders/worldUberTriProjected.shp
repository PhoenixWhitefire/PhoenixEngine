// This one needs **9** textures taps instead of `worldUber.shp`'s 3
// due to tri-planar mapping
// 3 for diffuse, 3 for specular, 3 for normal map (15/11/2024)
// 20/10/2024

{
    "Vertex": "worldUber.vert",
    "Fragment": "worldUber.frag",
    
    "InheritUniformsOf":  "worldUber",
    "Uniforms": {
      "UseTriPlanarProjection": true
    }
}